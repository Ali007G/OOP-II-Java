package practice;
import java.io.File;

public class renameFile {

	public static void main(String[] args) {
		File f = new File("D:\\Java\\practice\\class.txt");
		File r = new File("D:\\Java\\practice\\demo.txt");
		
		if(f.exists()) {
			System.out.println(f.renameTo(r));
		}
		else {
			System.out.println("File doesn't exist");
		}
	}
}
